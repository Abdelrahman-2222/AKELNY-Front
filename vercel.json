{
  "buildCommand": "npm install --include=dev && ng build --configuration=production",
  "outputDirectory": "dist/AKELNY-Front/browser",
  "rewrites": [
    {
      "source": "/uploads/(.*)",
      "destination": "http://akelni.tryasp.net/uploads/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "http://akelni.tryasp.net/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
